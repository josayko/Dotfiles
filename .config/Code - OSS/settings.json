{
    "workbench.startupEditor": "newUntitledFile",
    "window.menuBarVisibility": "toggle",
    "editor.fontFamily": "'DejaVuSansMono Nerd Font Mono', 'monospace', 'Droid Sans Fallback'",
    "editor.fontSize": 13,
    "workbench.colorTheme": "Palenight Theme",
    "keyboard.dispatch": "keyCode",
    "vim.useCtrlKeys": true,
    "editor.tabSize": 4,
    "[css]": {
		"editor.tabSize": 2
	},
	"[html]": {
		"editor.tabSize": 2
	},
	"[javascript]": {
		"editor.tabSize": 2
	},
	"[typescriptreact]": {
		"editor.tabSize": 2
    },
  "42header.username": "josaykos",
  "42header.email": "josaykos@student.42.fr",
  "editor.cursorBlinking": "solid",
  "extensions.autoUpdate": true,
  "telemetry.enableTelemetry": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/node_modules": true,
    "venv": true,
    "*.sublime-*": true,
    "env*": true
  }
}